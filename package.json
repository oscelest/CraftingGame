{
  "name": "pictologue",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "build-main": "tsc -p ./main/tsconfig.json -watch",
    "build-renderer": "tsc -p ./renderer/tsconfig.json -watch",
    "start": "electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.0.11",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "next": "^8.1.1-canary.64",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.135",
    "@zeit/next-less": "^1.0.1",
    "electron-is-dev": "^1.0.1",
    "electron-next": "^3.1.5",
    "less": "^3.9.0",
    "lodash": "^4.17.11"
  }
}
